{
  "name": "tpulse",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
    "format": "prettier '**/*.{ts,tsx}' --write",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "barrels": "barrelsby --config .barrelsby.json -q",
    "clean:git": "git branch --merged >/tmp/merged-branches && nano /tmp/merged-branches && xargs git branch -D </tmp/merged-branches && git fetch --prune --all"
  },
  "dependencies": {
    "@primer/react": "^36.9.0",
    "@tauri-apps/api": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@tauri-apps/cli": "^1.5.9",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "barrelsby": "^2.8.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
